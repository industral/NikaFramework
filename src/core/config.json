{
  "runtime": {},
  "instances": {
    "layout": {},
    "page": {},
    "widget": {}
  },
  "conf": {
    "defaultPage": "Home",

    "pageURL": "/data/pages",
    "URLSuffix": "",
    "pageNameExtension": "json",
    "localization": true,
    "loadingMaskSelector": "#page-loading-mask",
    "def": {
      "xml": {
        "path": "templates",
        "suffix": "xml"
      },
      "html": {
        "path": "templates",
        "name": "index",
        "suffix": "xhtml"
      },
      "svg": {
        "path": "svg",
        "suffix": "svg"
      },
      "css": {
        "path": "styles"
      },
      "js": {
        "name": "scripts"
      },
      "data": {
        "path": "data",
        "suffix": "json"
      },
      "attr": {
        "lang": "data-nkf-lang-textcontent",
        "component": {
          "type": "data-nkf-component-type",
          "name": "data-nkf-component-name",
          "dynamic": "data-nkf-component-dynamic"
        },
        "state": "data-state"
      }
    },

    "storage": {
      "key": "ks",
      "usersettings": "UserSettings",
      "networkdata": "NetworkData"
    },

    "components": "components",

    "render": {
      "html": "__dom__",
      "xml": "__dom__",
      "svg": "__dom__",
      "data": "__json__",
      "body": {
        "selector": "body"
      },
      "layout": {
        "selector": "section"
      },
      "dynamic": {
        "selector": "#dynamic"
      },
      "mask": {
        "selector": "#mask"
      }
    },

    "classes": {
      "none": "hidden"
    }
  },

  "enumType": {
    "Component": {
      "layout": "layout",
      "page": "page",
      "widget": "widget",
      "component": "component",
      "context": "context"
    },
    "Data": {
      "html": "html",
      "css": "css",
      "data": "data",
      "svg": "svg",
      "xml": "xml"
    }
  },

  "def": {
    "component": {
      "action": {
        "rendered": "rendered",
        "state": "state"
      }
    }
  }
}
